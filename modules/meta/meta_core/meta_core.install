/**
 * Implements hook_enable().
 */
function meta_core_enable() {
	variable_set('meta_user_role_content_manager', 'Community Moderator');
}

/**
 * Implements hook_disable().
 */
function meta_core_disable() {
	variable_del('meta_user_role_content_manager');
}
